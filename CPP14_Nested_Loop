#include <iostream>

using namespace std;

int main()
{
    for(int i=1;i <=10;i++){
        cout<<"table perkalian " << i<< endl;
        for(int j = 1; j <= 10; j++){
            //Inner loop ini akan mengeksekusi 10 kali untuk setiap iterasi outer loop
            cout<<  i<< " X " << j << "= "<< (i*j) << endl;
        }
        cout<<endl;
    }
}
